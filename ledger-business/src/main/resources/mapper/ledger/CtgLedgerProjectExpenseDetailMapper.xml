    <select id="selectCtgLedgerProjectExpenseDetailList" parameterType="CtgLedgerProjectExpenseDetail" resultMap="CtgLedgerProjectExpenseDetailResult">
        <include refid="selectCtgLedgerProjectExpenseDetailVo"/>

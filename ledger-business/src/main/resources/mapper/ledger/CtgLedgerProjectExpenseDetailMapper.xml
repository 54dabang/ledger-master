            <if test="expenseReportNumber != null "> and expense_report_number = #{expenseReportNumber}</if>
            <if test="subjectName != null  and subjectName != ''"> and subject_name like concat('%', #{subjectName}, '%')</if>
            <if test="feeType != null  and feeType != ''"> and feeType like concat('%', #{feeType}, '%')</if>
            <if test="amount != null "> and amount = #{amount}</if>
